package com.salman;

public class StudentClass {
	String name;
	int prince;
	Agrigation agrigation;
	public StudentClass(String n, int p,Agrigation a) {
		this.name = n;
		this.prince = p;
        this.agrigation=a;
	}
	void display() {
		System.out.println(name+" "+prince);
	}

public static void main(String args[]) {
Agrigation obj=new Agrigation("Salman", 24, "bhatpara");
StudentClass studentClass=new StudentClass("Imran khan", 30);
System.err.println("---------- Salman-Details-------------------");
obj.printShow();
System.err.print("------Imran datails-----");
studentClass.display();
}
}